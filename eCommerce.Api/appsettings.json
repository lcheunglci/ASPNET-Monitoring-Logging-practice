{
    "Logging": {
        "Console": {
            "LogLevel": {
                "Default": "Information",
                "eCommerce": "Debug",
                "Microsoft.AspNetCore": "Warning"
            },
            "FormatterName": "json",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "HH:mm:ss ",
                "UseUtcTimestamp": true,
                "JsonWriterOptions": {
                    "Indented": true
                }
            }
        }//,
        //"Debug": {
        //    "LogLevel": {
        //        "Default": "Critical"
        //    }
        //}
    },
  "AllowedHosts": "*"
}
